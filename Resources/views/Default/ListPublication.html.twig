{% extends "SiteReverseBundle:Default:index.html.twig" %}

{% block head %}
{% block title %}<title> Publication | reverse-engineering.fr</title>{% endblock %}
{% endblock %}

{% block body %}
{% block navbar %}
{% include "SiteReverseBundle:Default:navbarP.html.twig" %}
{% endblock %}
      <div class="container-narrow">
      <div class="row-fluid">
      <div class="jumbotron">
<center>

<h1 class="text-info"> {{ site.publicationTitle }} </h1>
<br><br>

{% block breadcrumbTop %}
{% include "SiteReverseBundle:Default:breadcrumbP.html.twig" %}
{% endblock %}


{% block Thumbnails %}

<ul class="thumbnails">
{% for publication in publication|reverse %}

<div>
  <li class="span6">
                <div class="thumbnail">
<h3> My {{ publication.titre }} {{ site.publicationtitle }} <br></h3>
    <img src="https://pbs.twimg.com/media/BK1hIu-CUAAb5gm.png" alt="300x200" style="width: 400px; height: 300px; "$
                  <div class="caption">
<h3> <small class="muted">{{ publication.date|date }}</small></h3>

<p><a href="{{ path("re_VoirPublication", { id: publication.id }) }}" class="btn">
{{ publication.titre }}</a></p>
                  </div>
                </div>
              </li>
</div>

{% endfor %}
</ul>
{% endblock %}

</center>
<br><br>
{% block breadcrumbBottom %}

{% include "SiteReverseBundle:Default:breadcrumbP.html.twig" %}

{% endblock %}

{% include "SiteReverseBundle:Default:footer.html.twig" %}


{% endblock %}

